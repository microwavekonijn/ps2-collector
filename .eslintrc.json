{
  "parser"       : "@typescript-eslint/parser",
  "extends"      : [
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType" : "module"
  },
  "rules"        : {
    "@typescript-eslint/quotes": ["error", "single"],
    "@typescript-eslint/explicit-member-accessibility": ["error"],
    "@typescript-eslint/camelcase": ["off"]
  }
}
