{
  "name": "ps2-alert-collector",
  "version": "1.0.0",
  "description": "Planetside 2 alert data collector using the PS2 census api.",
  "scripts": {
    "start": "node ./dist",
    "dev": "ts-node ./src",
    "build": "rm -rf ./dist && tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:up": "docker-compose up --build -d",
    "docker:logs": "docker-compose logs -f digbot",
    "docker:down": "docker-compose down",
    "up": "npm run docker:up && npm run docker:logs",
    "down": "npm run docker:stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microwavekonijn/ps2-alert-collector.git"
  },
  "keywords": [
    "websocket",
    "typescript",
    "ps2",
    "planetside"
  ],
  "author": "Microwavekonijn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/microwavekonijn/ps2-alert-collector/issues"
  },
  "homepage": "https://github.com/microwavekonijn/ps2-alert-collector#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "inversify": "^5.0.1",
    "ioredis": "^4.17.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.8",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/ioredis": "^4.16.3",
    "@types/lodash": "^4.14.152",
    "@types/mongodb": "^3.5.20",
    "@types/node": "^13.13.8",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  }
}
